Usuario.java

public class Usuario {
  protected int id;
            protected String nombre;
                protected String email;

                    public Usuario(int id, String nombre, String email) {
                                this.id = id;
                                        this.nombre = nombre;
                                                this.email = email;
                    }

                        public String getNombre() {
                                    return nombre;
                        }
}
                        }
                    }
}